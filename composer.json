{
    "name": "sedp-mis/base-api",
    "description": "Abstraction for api resources in laravel",
    "keywords": ["api", "base-api", "laravel-api", "api resources"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arjon Jason Castro",
            "email": "ajcastro29@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "4.2.11",
        "sedp-mis/lib": "2.*",
        "sedp-mis/base-repository": "1.0.x-dev",
        "satooshi/php-coveralls": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "1.6.*"
    },
    "autoload": {
        "psr-4": {
            "SedpMis\\BaseApi\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "classes/",
            "app/database/migrations",
            "app/database/seeds"
        ]
    },
    "minimum-stability": "stable"
}
